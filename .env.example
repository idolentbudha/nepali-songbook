# Expo public environment variables
# Copy this file to .env and adjust values as needed.
# Expo CLI will load EXPO_PUBLIC_* vars from .env at dev/build time.

# Enable the client-only stubbed Search Online screen
EXPO_PUBLIC_SEARCH_ONLINE=false

# Google Custom Search (client preview only; prefer backend)
# DO NOT commit real keys to source control.
EXPO_PUBLIC_GOOGLE_CSE_KEY=
EXPO_PUBLIC_GOOGLE_CSE_CX=

# Comma-separated list of sites to search
# Example: example.com,another.com,third.net
EXPO_PUBLIC_SEARCH_SITES=
